{
  // ===== RUST ANALYZER PERFORMANCE TUNING =====
  "rust-analyzer.server.extraEnv": {
    "RUST_LOG": "error",
    "RA_LOG": "error"
  },
  "rust-analyzer.cachePriming.enable": false,
  "rust-analyzer.cachePriming.numThreads": 2,
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.cargo.buildScripts.overrideCommand": null,
  "rust-analyzer.diagnostics.disabled": [
    "unresolved-proc-macro",
    "macro-error"
  ],
  "rust-analyzer.procMacro.ignored": {},
  "rust-analyzer.completion.limit": 25,
  "rust-analyzer.completion.postfix.enable": false,
  "rust-analyzer.hover.documentation.enable": true,
  "rust-analyzer.hover.links.enable": false,
  "rust-analyzer.imports.granularity.group": "module",
  "rust-analyzer.imports.prefix": "crate",
  "rust-analyzer.lens.references.adt.enable": false,
  "rust-analyzer.lens.references.enumVariant.enable": false,
  "rust-analyzer.lens.references.method.enable": false,
  "rust-analyzer.lens.references.trait.enable": false,
  "rust-analyzer.lens.implementations.enable": false,
  "rust-analyzer.lens.run.enable": false,
  "rust-analyzer.lens.debug.enable": false,
  "rust-analyzer.inlayHints.bindingModeHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "never",
  "rust-analyzer.inlayHints.discriminantHints.enable": "never",
  "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "never",
  "rust-analyzer.inlayHints.implicitDrops.enable": false,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "never",
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.reborrowHints.enable": "never",
  "rust-analyzer.inlayHints.renderColons": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.semanticHighlighting.strings.enable": false,
  "rust-analyzer.semanticHighlighting.punctuation.enable": false,
  "rust-analyzer.semanticHighlighting.punctuation.specialization.enable": false,
  "rust-analyzer.semanticHighlighting.operator.enable": false,
  "rust-analyzer.semanticHighlighting.operator.specialization.enable": false,

  // ===== MEMORY OPTIMIZATION =====
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "files.associations": {
    "*.rs": "rust",
    "Cargo.toml": "toml",
    "Cargo.lock": "toml"
  },

  // ===== WORKSPACE PERFORMANCE =====
  "files.watcherExclude": {
    "**/target/**": true,
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/.cargo/**": true,
    "**/Cargo.lock": false
  },
  "search.useReplacePreview": false,
  "search.showLineNumbers": false,
  "search.smartCase": true,
  "search.followSymlinks": false,

  // ===== EDITOR OPTIMIZATION =====
  "editor.quickSuggestionsDelay": 100,
  "editor.suggest.snippetsPreventQuickSuggestions": false,  "editor.suggest.filterGraceful": false,
  "editor.suggest.showWords": false,
  "editor.wordBasedSuggestionsMode": "currentDocument",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.suggest.showIcons": true,
  "editor.suggest.showSnippets": false,
  "editor.suggest.showUsers": false,
  "editor.suggest.showColors": false,
  "editor.suggest.showFiles": false,
  "editor.suggest.showReferences": false,
  "editor.suggest.showCustomcolors": false,
  "editor.suggest.showTypeParameters": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showOperators": false,
  "editor.suggest.showUnits": false,
  "editor.suggest.showValues": false,
  "editor.suggest.showConstants": true,
  "editor.suggest.showEnums": true,
  "editor.suggest.showEnumMembers": true,
  "editor.suggest.showKeywords": true,
  "editor.suggest.showText": false,
  "editor.suggest.showClasses": true,
  "editor.suggest.showFunctions": true,
  "editor.suggest.showConstructors": true,
  "editor.suggest.showFields": true,
  "editor.suggest.showVariables": true,
  "editor.suggest.showInterfaces": true,
  "editor.suggest.showModules": true,
  "editor.suggest.showProperties": true,
  "editor.suggest.showStructs": true,

  // ===== RUST SPECIFIC OPTIMIZATIONS =====
  "[rust]": {
    "editor.rulers": [100],
    "editor.wordWrap": "off",
    "editor.detectIndentation": false,
    "editor.trimAutoWhitespace": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.autoSurround": "languageDefined",
    "editor.bracketPairColorization.enabled": false,
    "editor.guides.bracketPairs": false,
    "editor.matchBrackets": "always",
    "editor.showFoldingControls": "mouseover"
  },

  // ===== CARGO OPTIMIZATION =====
  "rust-analyzer.cargo.autoreload": true,
  "rust-analyzer.cargo.noDefaultFeatures": false,
  "rust-analyzer.cargo.allFeatures": false,
  "rust-analyzer.cargo.features": [],
  "rust-analyzer.cargo.target": null,
  "rust-analyzer.cargo.noSysroot": false,

  // ===== CHECK ON SAVE OPTIMIZATION =====
  "rust-analyzer.checkOnSave.enable": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.checkOnSave.extraArgs": [
    "--",
    "-W", "clippy::all",
    "-W", "clippy::pedantic"
  ],
  "rust-analyzer.checkOnSave.overrideCommand": null,
  "rust-analyzer.checkOnSave.allTargets": false,
  "rust-analyzer.checkOnSave.noDefaultFeatures": null,
  "rust-analyzer.checkOnSave.allFeatures": null,
  "rust-analyzer.checkOnSave.features": null,
  "rust-analyzer.checkOnSave.target": null,

  // ===== DISABLE UNNECESSARY FEATURES =====
  "breadcrumbs.symbolSortOrder": "name",
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showClasses": true,
  "breadcrumbs.showConstants": true,
  "breadcrumbs.showConstructors": true,
  "breadcrumbs.showEnums": true,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": true,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.showFunctions": true,
  "breadcrumbs.showInterfaces": true,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showMethods": true,
  "breadcrumbs.showModules": true,
  "breadcrumbs.showNamespaces": true,
  "breadcrumbs.showNull": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showObjects": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.showPackages": true,
  "breadcrumbs.showProperties": true,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showStructs": true,
  "breadcrumbs.showTypeParameters": false,
  "breadcrumbs.showVariables": true
}
